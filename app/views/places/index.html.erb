<% @places.each do |place| %>
  <div class="black_box col-xs-10 col-xs-offset-1">
    <h1><%= place.name %></h1>
    <i class="address"><%= place.address %></i><br /><br />
    <p class="descrip"><%= place.description %></p><br />
  </div>
<% end %>

<br style="clear:both;" />

<div class="paginate_text">
<%= will_paginate @places %>
</div>